es = (movie) => {\n    dispatch(removeFavorite(movie)).then(() => dispatch(getFavorites()));\n  };\n\n  return (\n    <ul className={styles.moviesGrid}>\n      {movies?.map((movie) => (\n        <FavoritesDetails\n          key={movie.id}\n          movie={movie}\n          handleRemoveFavorites={handleRemoveFavorites}\n        /> //el movie={movie} pasa por prop a FavoritesDetails\n      ))}\n    </ul>\n  );\n}\n","/home/fabian/Bootcamp/TMDB 2/client/src/pages/MovieDetails.jsx",["86","87"],"import axios from \"axios\";\nimport React, { useEffect, useState } from \"react\";\nimport { useParams } from \"react-router\";\nimport { Spinner } from \"../components/Spinner\";\nimport styles from \"./MovieDetails.module.css\";\nimport { GrFavorite } from \"react-icons/gr\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { addFavorite, getFavorites, removeFavorite } from \"../store/favorites\";\n\nconst MovieDetails = () => {\n  const { movie_id } = useParams();\n  const [isLoading, setIsLoading] = useState(true);\n  const [movie, setMovie] = useState(null);\n  const dispatch = useDispatch();\n\n  const user = JSON.parse(localStorage.getItem(\"user\"));\n\n  useEffect(() => {\n    setIsLoading(true);\n    axios\n      .get(\n        `https://api.themoviedb.org/3/movie/${movie_id}?api_key=859094381e307b3a810be68bf946419d&language=en-US`\n      )\n      .then((data) => {\n        setIsLoading(false);\n        setMovie(data.data);\n      })\n      .catch((err) => console.log(err));\n  }, [movie_id]);\n\n  if (isLoading) {\n    return <Spinner />;\n  }\n\n  if (!movie) return null;\n\n  const handleAddFavorite = (movie) => {\n    dispatch(addFavorite(movie)).then(() => dispatch(getFavorites()));\n  };\n\n  return (\n    <div className={styles.detailsContainer}>\n      <img\n        className={`${styles.col} ${styles.movieImage}`}\n        src={`https://image.tmdb.org/t/p/w600_and_h900_bestv2${movie.poster_path}`}\n        alt={movie.title}\n      />\n      <div className={`${styles.col} ${styles.MovieDetails}`}>\n        <h2 className={styles.firstItem}>\n          <strong>Title: </strong>\n          {movie.title}\n        </h2>\n        {\n          <p>\n            <strong>Genres: </strong>\n            {movie.genres\n              .map((genre) => genre)\n              .map((gen) => gen.name)\n              .join(\", \") + \".\"}\n          </p>\n        }\n        <p>\n          <strong>Description: </strong>\n          {movie.overview}\n        </p>\n        <p>\n          <strong>Rating: </strong>\n          {movie.vote_average.toFixed(1)}\n        </p>\n\n        <div className=\"controls\">\n          {user ? (\n            <button type=\"button\" className=\"btn btn-danger btn-sm\">\n              <GrFavorite onClick={() => handleAddFavorite(movie)} />\n            </button>\n          ) : null}\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default MovieDetails;\n","/home/fabian/Bootcamp/TMDB 2/client/src/pages/LandingPage.jsx",[],"/home/fabian/Bootcamp/TMDB 2/client/src/hooks/useInput.js",[],"/home/fabian/Bootcamp/TMDB 2/client/src/components/Search.jsx",["88"],"import React, { useState } from \"react\";\nimport styles from \"./Search.module.css\";\nimport { useNavigate } from \"react-router\";\n\nconst Search = () => {\n  const [searchMovies, setSearchMovies] = useState(\"\");\n  const navigate = useNavigate()\n\n  const handleSearch = (e) => {\n    setSearchMovies(e.target.value);\n  };\n\n  const handleSubmit = (e) => {\n    e.preventDefault()\n    navigate(`/search/${searchMovies}`)\n  };\n\n  return (\n    <>\n    <div className={styles.container}>\n      <div className={styles.title}>\n        <h2>Welcome</h2>\n        <h3>Millions of movies, TV shows and people to discover. Explore now.</h3>\n      </div>\n      <form className={styles.search} role=\"searc h\" onSubmit={handleSubmit}>\n        <input\n          onChange={handleSearch}\n          value={searchMovies}\n          className={